{
  "version": 2,
  "name": "kb-contract-system",
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "images/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/dashboard", "dest": "/dashboard.html" },
    { "src": "/reset", "dest": "/reset.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
} 